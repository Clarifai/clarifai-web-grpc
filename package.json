{
  "name": "clarifai-web-grpc",
  "version": "8.8.0",
  "description": "The official Clarifai gRPC-web client",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "repository": "https://github.com/Clarifai/clarifai-web-grpc",
  "author": "Clarifai Inc.",
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "lint": "standard index.ts",
    "clean": "rm -rf src/gen dist clarifai-web-grpc-*.tgz",
    "build": "yarn bufgen && tsc -b",
    "prepack": "yarn clean && yarn build",
    "release": "semantic-release",
    "prepare": "husky install",
    "bufgen": "buf generate"
  },
  "dependencies": {
    "@bufbuild/connect-web": "^0.1.0",
    "@bufbuild/protobuf": "^0.1.0"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-connect-web": "^0.1.0",
    "@bufbuild/protoc-gen-es": "^0.1.0",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@types/node": "~10.17.0",
    "husky": "^7.0.4",
    "semantic-release": "^18.0.1",
    "standard": "^16.0.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
