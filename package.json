  "dependencies": {
    "google-protobuf": "3.21.2",
    "grpc-web": "1.3.1"
  },
  "devDependencies": {
    "@types/google-protobuf": "~3.7.0",
    "@types/node": "~10.17.0",
    "standard": "^16.0.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
